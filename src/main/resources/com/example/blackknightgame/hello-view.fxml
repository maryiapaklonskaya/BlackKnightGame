<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="503.0" prefWidth="253.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/19" fx:controller="com.example.blackknightgame.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Text fx:id="setText" strokeType="OUTSIDE" strokeWidth="0.0" text="Insert New BlackKnight's Name" />
   <TextField fx:id="insertName" alignment="TOP_CENTER" promptText="Insert Knight's name" />
   <Button fx:id="startButton" mnemonicParsing="false" onAction="#onStartButtonClick" text="start game" />
   <Group>
      <children>
         <Text fx:id="blackKnightText1" layoutX="0.5" strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="207.0" />
         <Text fx:id="blackKnightText2" layoutX="0.5" layoutY="35.5" strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="209.0" />
         <Text fx:id="blackKnightText3" layoutY="71.0" strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="211.0" />
      </children>
   </Group>
   <Button fx:id="strikeButton" mnemonicParsing="false" onAction="#onStrikeButtonClick" />
</VBox>
